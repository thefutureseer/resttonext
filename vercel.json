{
  "version": 2,
  "builds": [
    { "src": "package.json", "use": "@vercel/next" }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  },
  "build": {
    "env": {
      "CUSTOM_BUILD_ENV_VAR": "value"
    }
  },
  "functions": {
    "api/**/*.ts": {
      "maxDuration": 10
    }
  },
  "cleanUrls": true,
  "trailingSlash": false
}